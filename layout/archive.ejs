<div class="archive">
	<% if (page.category) { %>
		<h2>Category: <%= page.category %></h2>
	<% } %>
	<% if (page.tag) { %>
		<h2>Tag: <%= page.tag %></h2>
	<% } %>
	<ul>
		<% page.posts.forEach(function(post) { %>
			<li>
				<span class="post-date">
					<%= date(post.date, "YYYY-MM-DD" ) %>
				</span>
				<a href="<%- url_for(post.path) %>">
					<%= post.title %>
				</a>
			</li>
		<% }) %>
	</ul>
</div>

<%- partial('_partial/paginator') %>